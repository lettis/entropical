@article{Hyeon08,
Author = {Hyeon, Changbong and Morrison, Greg and Thirumalai, D.},
Title = {{Force-dependent hopping rates of RNA hairpins can be estimated from
   accurate measurement of the folding landscapes}},
Journal = PNAS,
Year = {{2008}},
Volume = {{105}},
Number = {{28}},
Pages = {{9604-9609}},
Abstract = {{The sequence-dependent folding landscapes of nucleic acid hairpins
   reflect much of the complexity of biomolecular folding. Folding
   trajectories, generated by using single-molecule force-clamp
   experiments by attaching semiflexible polymers to the ends of hairpins,
   have been used to infer their folding landscapes. Using simulations and
   theory, we study the effect of the dynamics of the attached handles on
   the handle-free RNA free-energy profile F-eq degrees(z(m)), where z(m)
   is the molecular extension of the hairpin. Accurate measurements of
   F-eq degrees(z(m)) requires stiff polymers with small L/I-p, where L is
   the contour length of the handle, and I-p is the persistence length.
   Paradoxically, reliable estimates of the hopping rates can only be made
   by using flexible handles. Nevertheless, we show that the equilibrium
   free-energy profile F-eq degrees(z(m)) at an external tension f(m), the
   force (f) at which the folded and unfolded states are equally
   populated, in conjunction with Kranners' theory, can provide accurate
   estimates of the force-dependent hopping rates in the absence of
   handles at arbitrary values of f. Our theoretical framework shows that
   z(m) is a good reaction coordinate for nucleic acid hairpins under
   tension.}},
