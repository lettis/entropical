journal = JMB,
keywords = {Amyloid,Amyloid beta-Peptides,Amyloid beta-Peptides: chemistry,Amyloid: chemistry,Computer Simulation,Hydrogen Bonding,Kinetics,Models, Molecular,Peptide Fragments,Peptide Fragments: chemistry,Protein Binding,Protein Structure, Secondary},
month = may,
number = {4},
pages = {1202--13},
pmid = {17382346},
title = {{Dissociation of Abeta(16-22) amyloid fibrils probed by molecular dynamics}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17382346},
