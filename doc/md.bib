month = jan,
number = {1},
pages = {110--8},
pmid = {15526299},
title = {{Early events in protein aggregation: molecular flexibility and hydrophobicity/charge interaction in amyloid peptides as studied by molecular dynamics simulations}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15526299},
